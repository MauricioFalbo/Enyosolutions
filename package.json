{
  "name": "enyosolutions",
  "version": "1.0.0",
  "description": "Test enyosolutions",
  "main": "index.js",
  "author": "Mauricio Falbo",
  "license": "MIT",
  "scripts": {
    "start": "tsnd --respawn --transpile-only --ignore-watch node_modules --exit-child --clear -r tsconfig-paths/register src/infra/http/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint src --ext ts,tsx",
    "build": "rm -rf build && webpack",
    "ci:build": "yarn install && yarn test:coverage && yarn lint && yarn build",
    "knex:list": "npx knex --knexfile src/infra/knex/knexfile.ts migrate:list",
    "knex:latest": "npx knex --knexfile src/infra/knex/knexfile.ts migrate:latest",
    "knex:rollback": "npx knex --knexfile src/infra/knex/knexfile.ts migrate:rollback",
    "knex:make": "npx knex --knexfile src/infra/knex/knexfile.ts migrate:make",
    "knex:up": "npx knex --knexfile src/infra/knex/knexfile.ts migrate:up",
    "knex:down": "npx knex --knexfile src/infra/knex/knexfile.ts migrate:down",
    "knex:seed": "npx knex --knexfile src/infra/knex/knexfile.ts seed:make",
    "knex:seedRun": "npx knex --knexfile src/infra/knex/knexfile.ts seed:run",
    "build:babel": "rm -rf dist && babel src --extensions \".ts\" --out-dir dist --copy-files"
  },
  "dependencies": {
    "@types/xml2js": "^0.4.11",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "knex": "^2.2.0",
    "knex-on-duplicate-update": "^2.3.0",
    "knex-stringcase": "^1.4.6",
    "mysql": "^2.18.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.15.0",
    "@babel/node": "7.14.9",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-typescript": "7.15.0",
    "@swc/core": "^1.2.155",
    "@swc/jest": "^0.2.20",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^27.5.1",
    "noop2": "^2.0.0",
    "prettier": "^2.6.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "pre-commit": [
    "lint"
  ]
}
